<h1>Listing questions</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questions.each do |question| %>
  <tr>
	<td><%= link_to question.title, "/questions/#{question.id}" %></td>
	<td><%= question.is_free %></td>
	<td><%= question.end_date %></td>
	<td><%= question.votes_count %></td>
	<td><%= question.answers_count %></td>
	<td><%= question.comments_count %></td>
	<td><%= question.created_at %></td>
  </tr>
<% end %>
</table>

<br />

